<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="translucent_black">#80000000</color>
    <string name="app_name">Library</string>
    <string name="default_progressbar">Default Progressbar:</string>
    <style name="CustomDialog" parent="@android:style/Theme.Light.NoTitleBar">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>
    <style name="CustomDialogTranslucent" parent="@android:style/Theme.Light.NoTitleBar">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@color/translucent_black</item>
    </style>
    <style name="MyCustomDialog" parent="@android:style/Theme.Holo.Light.Dialog">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>
    <declare-styleable name="ProgressWheel">
        <attr format="boolean" name="matProg_progressIndeterminate"/>
        <attr format="color" name="matProg_barColor"/>
        <attr format="color" name="matProg_rimColor"/>
        <attr format="dimension" name="matProg_rimWidth"/>
        <attr format="float" name="matProg_spinSpeed"/>
        <attr format="integer" name="matProg_barSpinCycleTime"/>
        <attr format="dimension" name="matProg_circleRadius"/>
        <attr format="boolean" name="matProg_fillRadius"/>
        <attr format="dimension" name="matProg_barWidth"/>
        <attr format="boolean" name="matProg_linearProgress"/>
    </declare-styleable>
</resources>